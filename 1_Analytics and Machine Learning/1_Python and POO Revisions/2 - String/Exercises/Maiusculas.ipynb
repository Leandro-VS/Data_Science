    "def maiusculas(frase):\n",
    "    x = []\n",
    "    d = \"\"\n",
    "    frase.split()\n",
    "    for i in range(len(frase)):\n",
    "        if ord(frase[i]) == ord(frase[i].upper()) and ord(frase[i]) > 65:\n",
    "            x.append(frase[i])\n",
    "    \n",
    "    for i in range(len(x)):\n",
    "        d += x[i]\n",
    "    return d"
   ]
